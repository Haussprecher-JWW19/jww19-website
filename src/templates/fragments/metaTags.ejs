<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta charset="UTF-8">
<meta name="description" content="<%= description %>">
<meta name="robots" content="index, follow">
<meta property="og:title" content="<%= title %>" />
<meta property="og:site_name" content="<%= title %>" />
<meta property="og:type" content="<%= openGraphType %>" />
<% if (openGraphType=== "article") { %>
    <meta property="article:published_time" content="2022-12-26" />
    <meta property="article:modified_time" content="<%= new Date().toISOString().substring(0, 10) %>" />
<% } %>
<meta property="og:updated_time" content="<%= new Date().toISOString().substring(0, 10) %>" />
<meta property="og:url" content="<%= `https://www.jww19.de/${lang}/${pageName}.html`%>" />
<meta property="og:description" content="<%= description %>" />
<meta property="og:image" content="<%= openGraphImageLink %>" />
<meta property="og:locale" content="<%= lang == 'de' ? 'de_DE' : 'en_GB' %>" />
<meta property="og:locale:alternate" content="<%= lang == 'de' ? 'en_GB' :  'de_DE' %>" />